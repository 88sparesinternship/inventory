<form {{action 'addKaryawan' karyawan on='submit'}}>
    Nama Karyawan : {{input value=karyawan.nama_karyawan}} <br>
    
    <button class="btn btn-success" type="submit">add</button>
</form>
<table>
    <tr>
        <th>Nama karyawan</th>
        <th>Action</th>
        
    </tr>
    {{#each model as |item|}}
    <tr>
        <td>{{item.nama_karyawan}}</td>
        <td><button class="btn btn-danger" {{action 'deleteKaryawan' item.id}}>Delete</button></td>
    </tr>
    {{/each}}
</table>